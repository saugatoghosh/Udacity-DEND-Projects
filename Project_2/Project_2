    "## Query 2: Returns only the following: name of artist, song (sorted by itemInSession) and user (first and last name)\\\n",
    "## for userid = 10, sessionid = 182\n",
    "query2 = \"CREATE TABLE IF NOT EXISTS song_playlist_session \"\n",
    "query2 = query2 + \"(userId int, sessionId int, itemInSession int, artist text, song text,\\\n",
    "                  firstName text, lastName text, PRIMARY KEY (userId, sessionId, itemInSession))\"\n",
    "try:\n",
    "    session.execute(query2)\n",
    "except Exception as e:\n",
    "    print(e)\n"
   ]
  },
